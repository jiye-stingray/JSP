function joinCheck(){
	if(orderfrm.orderno.value.length==0){
		alert("주문번호가 입력되지 않았습니다")
		return false;
	}
	if(orderfrm.custno.value.length==0){
		alert("고객번호가 입력되지 않았습니다");
		return false;
	}
	if(orderfrm.custname.value.length==0){
		alert("고객이름이 입력되지 않았습니다");
		return false;
	}
	if(orderfrm.menuno.value.length==0){
		alert("메뉴번호가 입력되지 않았습니다");
		return false;
	}
	
	return true;
}